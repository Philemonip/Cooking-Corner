      <!-----------------------------------JumboTron---------------------------------------------------------->
      <div class="cover container-fluid text-black">
        <div
          class="cover-content d-flex flex-column justify-content-center align-items-center"
        >
          <img id="one" src="{{ image }}" alt="" class="rounded-circle" />

          <h1 id="firsth" class="display-5 text-capitalize pt-4 pt-md-0">
            {{  Ethnicity of cuisine  }}
          </h1>
          <h6 id="buddy" class="pt-3 h4">Recipes for best {{ ethnicity of dish}} cuisine</h6>
        </div>
      </div>
