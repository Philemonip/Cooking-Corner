</header>



<!----------------------------------Examples of Category of Cuisine --------------------------------------------------------------->

{{!-- {{#each example.cuisine }} --}}
{{#each recipes }}
<div class="container">

    <div class="row">



        <div class="col-md-7">
            <a href="../recipe/{{this.api_id}}">
                <img class="img-fluid rounded mb-3 mb-md-0" src="{{ image_path}} " alt="Photo unavailable">
            </a>
        </div>
        <div class="col-md-5">
            <a href="../recipe/{{this.api_id}}">
                <h3>{{ title }}</h3>
            </a>
            <h5>By: {{author}}</h5>
            <p>
                {{this.rating}}
                <i class="fab fa-pagelines"></i>
            </p>
            <div class="col">
                <button id="post-bookmark" type="button" class="bookmark-add-btn btn btn-primary" name="add-bookmark"
                    data-recipeid={{this.api_id}}>Bookmark</button>
            </div>
            <p>{{{summary}}}</p>

        </div>
    </div>
    <!-- /.row -->

</div>
{{/each}}