  <!--   ----------------------------------The Search Bar --------------------------------------------------------->

  <div class="search">
    <form class="d-flex justify-content-center">
      <input class="typeahead form-control me-2" type="search" placeholder="Search recipes" aria-label="Search">
    </form>
  </div>



    <!----------------------------------Examples of Category of Cuisine --------------------------------------------------------------->

{{#each example.cuisine }}
<div id="container">

  <!-- Start Product details -->
<div class="product-details">

      <!-- 	Product Name -->
    <h1>{{ name of dish }}</h1>
    <p>{{author of dish}}</p>
    <span class="hint-star star">
     {{ user rating }}
    </span>


      <p class="information">"{{ info descripton of the dish}} "</p>

  </div>

  <!-- 	Start product image & Information -->

  <div class="product-image">

    <img src="{{ image of the dish }}" alt="">

  <div class="info">
    <h2 id="receipeid">{{ name of receipe}}</h2>
    <ul>
      <li><strong>{{ user rating }}</li>
      <li><strong>{{ bookmark }}</li>
     
    </ul>
  </div>
  </div>
  <!--  End product image  -->


  </div>
{{/each}}

   
    <!-------------------------------------------Footer --------------------------------------------------------------------->
    <footer class="text-center bg-dark py-3">
      <p class="text-white mb-0">Receipes under Xccelerate</p>
    </footer>

